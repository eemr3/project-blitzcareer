{
  "name": "back-end",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/api/server.js",
    "drop": "cross-env NODE_ENV=development npx sequelize-cli db:drop",
    "restore": "cross-env NODE_ENV=development npx sequelize-cli db:create && NODE_ENV=development npx sequelize-cli db:migrate",
    "start": "node src/api/server.js",
    "pretest": "cross-env NODE_ENV=test npx sequelize-cli db:create && cross-env NODE_ENV=test npx sequelize-cli db:migrate",
    "test": "cross-env NODE_ENV=test mocha src/tests/**/*$NAME*.test.js --exit",
    "test:coverage": "npm run pretest && cross-env NODE_ENV=test nyc mocha src/tests/**/*$NAME*.test.js --exit",
    "posttest": "cross-env NODE_ENV=test npx sequelize-cli db:migrate:undo:all",
    "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json ."
  },
  "keywords": [],
  "author": "Emerson Moreira",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "eslint-config-trybe-backend": "1.0.3",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "sequelize": "^6.21.0",
    "sequelize-test-helpers": "^1.4.2"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.1",
    "frisby": "2.1.2",
    "jest": "26.0.1",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.18",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.4.1",
    "sinon": "^14.0.0",
    "supertest": "4.0.2"
  }
}
